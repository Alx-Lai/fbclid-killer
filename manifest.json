{
  "name": "fbclid Killer",
  "description": "Remove annoying fbclid from your browser",
  "version": "0.1",
  "manifest_version": 3,
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "exclude_matches": [
        "https://www.facebook.com/*"
      ]
    }
  ]
}
